{
  "name": "srujan",
  "displayName": "Srujan (सृजन)",
  "description": "Adds syntax highlighting and other features for the language : सृजन ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "srujan",
      "aliases": ["सृजन ", "srujan"],
      "extensions": ["सृ"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "srujan",
      "scopeName": "source.सृ",
      "path": "./syntaxes/srujan.tmLanguage.json"
    }]
  },
  "dependencies": {
    "vscode-languageserver": "^7.0.0",
    "vscode-languageserver-textdocument": "^1.0.1"
  }
}
